<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:presentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"  
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"  
	mc:Ignorable="presentationOptions"
	>

	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="SystemMap.xaml" />
	</ResourceDictionary.MergedDictionaries>

	<SolidColorBrush x:Key="DataValuePanelBrush" Color="{StaticResource VeryLightBlue}" presentationOptions:Freeze="true" />
	<Pen x:Key="DataValuePanelPen" Brush="{StaticResource CoolBlueBrush}" Thickness="1" presentationOptions:Freeze="true" />
	
</ResourceDictionary>